<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#232121">


    <TextView
        android:id="@+id/textView"
        android:layout_width="231dp"
        android:layout_height="44dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="88dp"
        android:layout_marginRight="88dp"
        android:gravity="center"
        android:text="관심목록"
        android:textColor="@color/white"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:text="  ━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━"
        android:textColor="#6e6e6e"
        android:textSize="9dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp" />


    <Button
        android:id="@+id/my_dang_guen_like_all_button_white"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/drawable_my_dang_guen_like_white_line"
        android:text="전 체"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/my_dang_guen_like_all_button_select"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/drawable_my_dang_guen_like_gray_line"
        android:text="전 체"
        android:textColor="#787878"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/my_dang_guen_like_all_button_white"
        app:layout_constraintStart_toStartOf="@+id/my_dang_guen_like_all_button_white"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/my_dang_guen_like_product_button_white"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/drawable_my_dang_guen_like_white_line"
        android:text="중고거래"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/my_dang_guen_like_product_button_select"
        app:layout_constraintStart_toStartOf="@+id/my_dang_guen_like_product_button_select"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/my_dang_guen_like_product_button_select"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/drawable_my_dang_guen_like_gray_line"
        android:text="중고거래"
        android:textColor="#787878"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/my_dang_guen_like_all_button_white"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/my_dang_guen_like_dong_nae_life_button_white"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/drawable_my_dang_guen_like_white_line"
        android:text="동네생활"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/my_dang_guen_like_dong_nae_life_button_select"
        app:layout_constraintStart_toStartOf="@+id/my_dang_guen_like_dong_nae_life_button_select"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/my_dang_guen_like_dong_nae_life_button_select"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/drawable_my_dang_guen_like_gray_line"
        android:text="동네생활"
        android:textColor="#787878"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/my_dang_guen_like_product_button_white"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:ignore="MissingConstraints" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/my_dang_guen_like_recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="150dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>